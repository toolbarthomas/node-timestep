{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": true,
    "lib": [
      "dom",
      "ES2020",
      "esnext"
    ],
    "target": "ES2020",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "System",
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist",
    "plugins": [],
    "rootDir": "./src",
    "strict": true,
    "types": [
      "node",
    ],
    "typeRoots": [
      "./src",
      "node_modules/@types",
    ],
  },
  "include": [
    "src/index.ts",
    "src/_types.d.ts"
  ],
  "exclude": [],
}